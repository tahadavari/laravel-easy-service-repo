{
  "name": "taha-davari/laravel-easy-service-repo",
  "description": "A Laravel package to easily generate service and repository classes with interfaces.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TahaDavari\\LaravelEasyServiceRepo\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Taha Davari",
      "email": "tahadavari1381@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "TahaDavari\\LaravelEasyServiceRepo\\Providers\\ServiceServiceProvider",
        "TahaDavari\\LaravelEasyServiceRepo\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
